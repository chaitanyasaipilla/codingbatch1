import java.util.*;
class hashmapsorted{
public static void main(String args[]){
HashMap<Integer,String> A=new HashMap<Integer,String>();
Scanner sc=new Scanner(System.in);
System.out.print("Enter the KV:");
int n=sc.nextInt();
for(int i=0;i<n;i++){
int key=sc.nextInt();
String value=sc.next();
A.put(key,value);}
System.out.println(A);
TreeMap<Integer,String>S1=new TreeMap<Integer,String>(A);
for(Integer i:S1.keySet()){
System.out.println(i+"="+S1.get(i)+" ");
}
}
}
