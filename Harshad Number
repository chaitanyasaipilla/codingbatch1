import java.util.*;
public class HarshadNumber{
public static void main(String[] args){
Scanner sc=new Scanner(System.in);
System.out.println("Enter a number: ");
int n=sc.nextInt();
int sum=0;
int temp =n;
while(n>0)
{
int remainder=n%10;
sum=sum+remainder;
n=n/10;
}
if(temp%sum==0)
System.out.println(temp+" is Harshal Number");
else
System.out.println(temp+" is not Harshal Number");
}
}
