import java.util.*;
class EvenOdd
{
public static void main (String args[])
{
String str;
Scanner sc=new Scanner(System.in);
System.out.println("Enter a string");
str=sc.next();
int n=str.length();
System.out.print("odd: ");
for(int i=0;i<n;i++)
{
if(i%2==0)
System.out.print(str.charAt(i));
}
System.out.print("even: ");
for(int i=0;i<n;i++)
{        
if(i%2!=0)
System.out.print(str.charAt(i));
}
}
}
