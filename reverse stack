import java.util.*;
class reversestack{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
Stack<Character> s1=new Stack<Character>();
System.out.println("Enter String:");
String str=sc.next();
char[] ch = str.toCharArray();
int n = ch.length;
for(int i=0;i<n;i++)
{s1.push(ch[i]);}
for(int i=0;i<n;i++)
System.out.print(s1.pop());
}
}
