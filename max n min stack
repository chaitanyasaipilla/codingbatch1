import java.util.*;
class MaxMinstack{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
Stack <Integer> s1=new Stack<Integer>();
System.out.print("enter the size:");
int n=sc.nextInt();
System.out.print("enter "+n+" s1values:");
for(int i=0;i<n;i++)
{
s1.push(sc.nextInt());
}
System.out.print("Max ="+Collections.max(s1));
System.out.print("Min ="+Collections.min(s1));
}
}
